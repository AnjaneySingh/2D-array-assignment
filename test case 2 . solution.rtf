// Function to convert a number (string) from base 'base' to decimal\
int convertToDecimal(char value[], int base) \{\
    int len = strlen(value);\
    int result = 0;\
    int power = 1; // base^0\
\
    for (int i = len - 1; i >= 0; i--) \{\
        int digit;\
\
        if (value[i] >= '0' && value[i] <= '9')\
            digit = value[i] - '0';\
        else\
            digit = value[i] - 'A' + 10; // for bases > 10\
\
        result += digit * power;\
        power *= base;\
    \}\
    return result;\
\}\
\
